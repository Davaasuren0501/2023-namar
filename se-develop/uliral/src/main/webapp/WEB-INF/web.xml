<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>uliral</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>

		  
   <servlet>
	    <servlet-name>ControllerMain</servlet-name>
	    <servlet-class>controller.MyController</servlet-class>
	</servlet>
	
	<servlet-mapping>
	    <servlet-name>ControllerMain</servlet-name>
	    <url-pattern>/controller/MyController</url-pattern>
	</servlet-mapping>
  
   <servlet>
	    <servlet-name>ControllerProductServlet</servlet-name>
	    <servlet-class>controller.product.ProductServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
	    <servlet-name>ControllerProductServlet</servlet-name>
	    <url-pattern>/controller/product/ProductServlet</url-pattern>
	</servlet-mapping>
	
   <servlet>
	    <servlet-name>ControllerSupplierServlet</servlet-name>
	    <servlet-class>controller.supplier.SupplierServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
	    <servlet-name>ControllerSupplierServlet</servlet-name>
	    <url-pattern>/controller/supplier/SupplierServlet</url-pattern>
	</servlet-mapping>
	
   <servlet>
	    <servlet-name>ControllerProductEdit</servlet-name>
	    <servlet-class>controller.product.ProductEdit</servlet-class>
	</servlet>
	
	<servlet-mapping>
	    <servlet-name>ControllerProductEdit</servlet-name>
	    <url-pattern>/controller/product/ProductEdit</url-pattern>
	</servlet-mapping>
</web-app>